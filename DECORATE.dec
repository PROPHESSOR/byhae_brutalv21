// Unconscious Doomguy
ACTOR SleepingMarine 587
{
//$Category PowerUps
    Radius 24
    Height 48
	deathheight 48
    Speed 0
	Mass 999999999999999999
    Health 100
    gravity 0.5
	Decal BloodSuper
    +SHOOTABLE
    +SOLID
	//MONSTER
	//-COUNTKILL
    Painchance 255
    DamageType Taunt
    damagefactor "Trample", 0.0
    damagefactor "Cut", 10.0
    damagefactor "Taunt", 0.0
    damagefactor "headcut", 0.0
    damagefactor "KillMe", 0.0
    damagefactor "Fatality", 1.0
    damagefactor "Melee", 1.0
    damagefactor "Kick", 1.0
	DeathSound "*death"
	PainSound "*pain50"
    BloodType "Nashgore_Blood"
    States
    {
    Spawn:
        C7PT ABCB 25
        Loop

    SetFree:
    Death.Cut:
    Death.Melee:
	Death.SuperPunch:
    Death.Fatality:
	Death.ExtremePunches:
    Death.Kick:
	Death.Slide:
		TNT1 A 0 A_Playsound ("TAUNT")
		TNT1 A 0 A_ChangeFLag ("NOPAIN", 1)
        MLST ABCDE 6
        TNT1 A 0 A_NoBlocking
        TNT1 A 0 A_SpawnItem("Ally")
        TNT1 A 0
		TNT1 A 0 A_ChangeFLag ("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag ("SOLID", 0)
		TNT1 A 0 A_GiveToTarget("NumberOfAllies", 1)
        TNT1 A 0
        Stop
    Pain:
        TNT1 A 0
        Goto Spawn
    Death:
        TNT1 A 0 A_Scream
        TNT1 A 0 A_NoBlocking
		C7PT EFGHIJ 7
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
        C7PT J -1
        Stop
    }
}
