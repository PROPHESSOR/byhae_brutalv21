// Duke Alien Barrel
Actor FuelCell 590
{
//$Category TechStuff
height 48
radius 8
+SOLID
+SHOOTABLE
+NOBLOOD
Health 25
Scale 0.5
states
	{
	Spawn:
	    ABAR ABCB 5 bright
		Loop
	Death:
	    TNT1 A 0
		THRF A 0 A_SpawnItemEx ("Rocket",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0)
		THRF A 0 A_SpawnItemEx ("Detonator",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0)
		Stop
	}
}

