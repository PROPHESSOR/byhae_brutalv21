//KEY CARDS

Actor GreenCard : DoomKey 10000
{
	//$Category Keys
	//$Title "Green Keycard"
	Inventory.Pickupmessage "Picked up a green keycard."
	Inventory.Icon "STKEYS18"
	States
	{
	Spawn:
		GKEY A 10
		GKEY B 10 bright
		loop
	}
}

